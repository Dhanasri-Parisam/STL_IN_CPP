//Queue in cpp:

#include<iostream>
#include<queue>
using namespace std;
int main(){
    queue<int> st;
    st.push(1);
    st.push(2);
    st.push(3);
    st.push(10);
    cout<<st.size()<<"\n";
    cout<<st.front()<<"\n";
    cout<<st.back()+1<<"\n";
    st.pop();
    cout<<st.empty();
}

//priority_Queue_in_cpp:

